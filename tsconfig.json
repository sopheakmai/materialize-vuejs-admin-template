{
  "compilerOptions": {
    "target": "esnext",
    "jsx": "preserve",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "useDefineForClassFields": true,
    "baseUrl": "./",
    "module": "esnext",
    "moduleResolution": "bundler",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@themeConfig": [
        "themeConfig.ts"
      ],
      "@layouts/*": [
        "src/@layouts/*"
      ],
      "@layouts": [
        "src/@layouts"
      ],
      "@core/*": [
        "src/@core/*"
      ],
      "@core": [
        "src/@core"
      ],
      "@images/*": [
        "src/assets/images/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@axios": [
        "src/plugins/axios"
      ],
      "@validators": [
        "src/@core/utils/validators"
      ]
    },
    "resolveJsonModule": true,
    "types": [
      "vite/client",
      "vite-plugin-pages/client",
      "vite-plugin-vue-layouts/client",
      "unplugin-vue-define-options/macros-global"
    ],
    "strict": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "skipLibCheck": true
  },
  "include": [
    "vite.config.*",
    "types/env.d.ts",
    "types/shims.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "themeConfig.ts",
    "types/auto-imports.d.ts",
    "types/components.d.ts"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}
